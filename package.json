{
  "name": "react-base",
  "version": "2.0.0",
  "authors": [
    "Marat Fakhreev <marat.fakhreev@flatstack.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:fs/react-base.git"
  },
  "scripts": {
    "stylelint": "stylelint './app/**/*.css'",
    "stylelint-fix": "stylefmt --recursive './app/**/*.css'",
    "eslint": "eslint 'server.js' './app' './config' --ext .js,.jsx",
    "eslint-fix": "yarn eslint -- --fix",
    "lint": "yarn stylelint && yarn eslint",
    "jest": "jest",
    "test": "yarn lint && yarn jest",
    "start": "node ./server"
  },
  "license": "MIT",
  "engines": {
    "node": "^9.0.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^4.0.0-beta.18",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-s-alert": "^1.4.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
